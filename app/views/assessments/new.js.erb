var html = "<%= j render('form') %>";
var assessmentModal = $("#assessment-modal");
if (assessmentModal.length == 1) {
	assessmentModal.modal("hide");
	assessmentModal.on('hidden.bs.modal', function(e){
		$(e.target).remove();
		$("body").append(html);
		$("#assessment-modal").modal({show: true});
	});
}else{
	$("body").append(html);
	$("#assessment-modal").modal({show: true});
}