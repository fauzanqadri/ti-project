var html = "<%= j render('form') %>";
var importModal = $("#import-modal");
if (importModal.length == 1) {
	importModal.modal("hide");
	importModal.on('hidden.bs.modal', function(e){
		$(e.target).remove();
		$("body").append(html);
		$("#import-modal").modal({show: true});
	});
}else{
	$("body").append(html);
	$("#import-modal").modal({show: true});	
}