= form_for @examiner do |f|
  - if @examiner.errors.any?
    #error_explanation
      %h2= "#{pluralize(@examiner.errors.count, "error")} prohibited this examiner from being saved:"
      %ul
        - @examiner.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :sidang_id
    = f.number_field :sidang_id
  .field
    = f.label :lecturer_id
    = f.number_field :lecturer_id
  .actions
    = f.submit 'Save'
