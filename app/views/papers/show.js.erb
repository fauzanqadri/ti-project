var html = "<%= j render('show') %>";
var paperModal = $("#paper-modal");
if (paperModal.length == 1) {
	paperModal.modal("hide");
	paperModal.on('hidden.bs.modal', function(e){
		$(e.target).remove();
		$("#papers footer").append(html);
		$("#paper-modal").modal({show: true});
	});
}else{
	$("#papers footer").append(html);
	$("#paper-modal").modal({show: true});	
}