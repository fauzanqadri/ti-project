var html = "<%= j render('form') %>";
var paperModal = $("#newPaper-modal");
if (paperModal.length == 1) {
	paperModal.modal("hide");
	paperModal.on('hidden.bs.modal', function(e){
		$(e.target).remove();
		$("body").append(html);
		$("#newPaper-modal").modal({show: true});
	});
}else{
	$("body").append(html);
	$("#newPaper-modal").modal({show: true});	
}