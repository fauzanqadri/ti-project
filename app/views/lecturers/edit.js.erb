var html = "<%= j render('form') %>";
var lecturerModal = $("#lecturer-modal");
if (lecturerModal.length == 1) {
	lecturerModal.modal("hide");
	lecturerModal.on('hidden.bs.modal', function(e){
		$(e.target).remove();
		$("body").append(html);
		$("#lecturer-modal").modal({show: true});
	});
}else{
	$("body").append(html);
	$("#lecturer-modal").modal({show: true});	
}
$(".datepicker").datetimepicker({
	language: 'id',
	pickTime: false,
	icons: {
        date: "fa fa-calendar",
        up: "fa fa-arrow-up",
        down: "fa fa-arrow-down"
    }
});