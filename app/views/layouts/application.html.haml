!!!
%html{lang: "id"}
	%head
		%meta{content: "width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no", name: "viewport"}
		%title= full_title(yield :title)
		= stylesheet_link_tag    "application", :media => "all", "data-turbolinks-track" => true
		= javascript_include_tag hierarchical_part_url
		= javascript_include_tag "application", "data-turbolinks-track" => true
		= yield :specific_js
		= yield :bottom_js
		/[if lt IE 9]
			<script src="http://html5shim.googlecode.com/svn/trunk/html5.js" type="text/javascript"></script>
		= csrf_meta_tags
	%body
		= using_header
		#the-flash
			= render partial: 'layouts/shared/flash', locals: {flash: flash}
		.container
			- if user_signed_in?
				%input#identification-unique{type: "hidden", value: current_user.socket_identifier}
			= yield
			%small
				&copy; 2014 SIMPS By
				%a{:href => "https://www.twitter.com/fauzanqadri"} @fauzanqadri
				
