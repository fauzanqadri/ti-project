#seminar-modal.modal.fade{tabindex: "-1", role: "dialog", "aria-labelledby" => "seminarModalLabel", "aria-hidden" => true, "data-backdrop" => "static", "data-keyboard" => "true"}
	.modal-dialog
		.modal-content
			.modal-header
				%button.close{type: "button", "data-dismiss" => "modal", "aria-hidden" => true} &times;
				%h4#seminarModalLabel.modal-title Pendaftaran Seminar
			= form_for [@skripsi, @seminar], remote: "true", html: {role: "form", class: "form-horizontal"} do |f|
				.modal-body
					= render 'layouts/shared/errors', :object => f.object
					.form-group
						= f.label :tanggal_dilaksanakan, class: "col-sm-3 control-label"
						.col-sm-9
							.input-group.date.datepicker
								= f.text_field :start, class: "form-control", placeholder: "Rencana Tanggal pelaksanaan", "data-format" => "YYYY-MM-DD hh:mm"
								%span.input-group-addon
									%span.fa.fa-calendar
				.modal-footer
					%button.btn.btn-primary{type: "submit"}
						%i.fa.fa-upload
						Simpan
					%button.btn.btn-default{type: "button", "data-dismiss" => "modal"}
						%i.fa.fa-times
						Cencel