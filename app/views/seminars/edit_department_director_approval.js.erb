var html = "<%= j render('edit_department_director_approval_form') %>";
var conferenceModal = $("#conference-modal");
if (conferenceModal.length == 1) {
	conferenceModal.modal("hide");
	conferenceModal.on('hidden.bs.modal', function(e){
		$(e.target).remove();
		$("body").append(html);
		$("#conference-modal").modal({show: true});
	});
}else{
	$("body").append(html);
	$("#conference-modal").modal({show: true});
}
$(".datepicker").datetimepicker({
	language: 'id',
	pickTime: false,
	icons: {
        date: "fa fa-calendar",
        up: "fa fa-arrow-up",
        down: "fa fa-arrow-down"
    }
});