var html = "<%= j render('form') %>";
var reportModal = $("#newReport-modal");
if (reportModal.length == 1) {
	reportModal.modal("hide");
	reportModal.on('hidden.bs.modal', function(e){
		$(e.target).remove();
		$("body").append(html);
		$("#newReport-modal").modal({show: true});
	});
}else{
	$("body").append(html);
	$("#newReport-modal").modal({show: true});	
}