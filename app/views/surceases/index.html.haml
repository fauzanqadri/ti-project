- provide(:title, "Publish Skripsi / PKL")
- content_for :specific_js do
	= javascript_include_tag "surceases", "data-turbolinks-track" => true

#wraper
	%header
		%h1 Publish Pkl / Skripsi

	%section
		%table#surceases.table.table-bordered{"data-source" => "#{surceases_url(format: "json")}"}
			%thead
				%tr
					%th
						.text-center Judul Skripsi / Pkl
					%th
						.text-center Penulis
					%th
						.text-center Tipe
					%th
						.text-center Pembimbing
					%th
						.text-center Jenis Penanggunjawab
					%th
						.text-center Publish
					%th
						.text-center Action
			%tbody
	%footer